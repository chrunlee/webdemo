<script type="text/javascript" src="/plugins/byy/byy.js"></script>
<!-- 用户信息 -->
<script type="text/javascript">
var LoginUser;
{{if site.github}}
LoginUser = {
	id : '{{site.github.id}}',
	name : '{{site.github.name}}',
	blog : '{{site.github.blog || site.github.html_url || ""}}',
	avatar_url : '{{site.github.avatar_url}}'
};
{{/if}}
byy.setRequire({
	basePath : '/js/common/',
	modules : {
		common : 'common'
	}
});
</script>
